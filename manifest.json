{
    "manifest_version":  2,
    "name":  "DMCA Redirect",
    "version":  "1.3.0",
    "description":  "Suggests alternative searches when a DMCA notice is detected",
    "permissions":  [
                        "webRequest",
                        "storage",
                        "\u003call_urls\u003e"
                    ],
    "browser_action":  {
                           "default_icon":  "icon.png",
                           "default_popup":  "popup.html"
                       },
    "background":  {
                       "scripts":  [
                                       "js/background.js"
                                   ],
                       "type":  "module"
                   }
}
